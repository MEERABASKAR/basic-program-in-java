    Scanner obj = new Scanner(System.in);
      int a = obj.nextInt();
      System.out.println((char)a);
  }
}


//5 Update Counter
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner obj = new Scanner(System.in);
      int a = obj.nextInt();
      System.out.println(((a+5)*2)-3);
  }
}


// 6 ODD or EVEN
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner obj = new Scanner(System.in);
      int a = obj.nextInt();
      if(a%2==0){
        System.out.println("Even");
      }
        else{
        System.out.println("Odd");
        }
    
  }
}


//7  Greatest Among Three Numbers
import java.util.*;

public class Main {
    public static void main(String[] args) {
     Scanner obj = new Scanner(System.in);
     int a = obj.nextInt();
     int b = obj.nextInt();
     int c = obj.nextInt();
     if((a>b) && (a>c)){
       System.out.println(a);
     }
       else if((b>a) && (b>c)){
         System.out.println(b);
       }
         else{
           System.out.println(c);
         }
  }
}


//8  Character Digit to Integer
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner obj = new Scanner(System.in);
      char  a = obj.next().charAt(0);
      System.out.println((int)a-48);
  }
}


//9  Personalized Greeting
import java.util.*;

public class Main {
    public static void main(String[] args) {
     Scanner obj = new Scanner(System.in);
     String s = obj.next();
     System.out.println("Hi "+s+", welcome to the programming class!");
  }
}


//10 Calculate Total and Average Marks
import java.util.*;

public class Main {
    public static void main(String[] args) {
    Scanner obj = new Scanner(System.in);
    String s = obj.next();
    int a = obj.nextInt();
    int b = obj.nextInt();
    int c = obj.nextInt();
    int d = obj.nextInt();
    int e = obj.nextInt();
    int total = a+b+c+d+e;
    double avg = (total)/5.0;
    System.out.println("Student Name: " +s);
    System.out.println("Total Marks: " +total);
    System.out.printf("Average Marks: %.2f\n",avg);
  }
}

//11 Shape Area Calculation
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner obj = new Scanner(System.in);
      int a = obj.nextInt();
      int l = obj.nextInt();
      int b = obj.nextInt();
      int r = obj.nextInt();
      int B = obj.nextInt();
      int h = obj.nextInt();
      double pi=3.14;
      System.out.println("Area of Square="+(a*a));
      System.out.println("Area of Rectangle="+(l*b));
      System.out.printf("Area of Circle=%.2f\n",pi*Math.pow(r,2));
      System.out.println("Area of Triangle="+(int)((0.5)*B*h));
    }
}


// 12  Shape Perimeter Calculation Program
import java.util.*;

class ShapePerimeter {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int a = sc.nextInt();   
        int l = sc.nextInt();   
        int w = sc.nextInt();   
        int r = sc.nextInt();   
        int b1 = sc.nextInt();  
        int s = sc.nextInt();   
        int b2 = sc.nextInt();  

        int square = 4 * a;
        int rectangle = 2 * (l + w);
        double circle = 2 * 3.14 * r;
        int triangle = b1 + s + b2;

        System.out.println(square);
        System.out.println(rectangle);
        System.out.printf("%.2f\n", circle);
        System.out.println(triangle);
    }
}
